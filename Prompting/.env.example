# API Keys
#OPENAI_API_KEY=your_openai_api_key_here
#ANTHROPIC_API_KEY=your_anthropic_api_key_here
GROQ_API_KEY=your_groq_api_key_here
# Model Configuration
DEFAULT_MODEL=llama-3.3-70b-versatile
